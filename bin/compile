#!/usr/bin/env bash

# (c) 2017 XYSec Labs

indent() {
    sed -u 's/^/       /'
}

echo '-----> Downloading Syntribos payloads'

PAYLOADS_URL=https://github.com/openstack/syntribos-payloads/archive/master.zip

curl -sSL $PAYLOADS_URL | tar xz -C $BUILD_DIR/syntribos/payloads

echo $BUILD_DIR/syntribos/payloads | indent
